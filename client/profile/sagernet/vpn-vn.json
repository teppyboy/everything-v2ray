        {
          "outboundTag": "dns-out",
          "protocol": [
            "dns"
          ],
          "type": "field"
        },
        {
          "ip": [
            "35.215.183.30"
          ],
          "outboundTag": "direct",
          "type": "field"
        },
        {
          "ip": [
            "geoip:private"
          ],
          "outboundTag": "bypass",
          "type": "field"
        },
        {
          "ip": [
            "255.255.255.255"
          ],
          "outboundTag": "block",
          "type": "field"
        }
      ]
    },
    "stats": {}
