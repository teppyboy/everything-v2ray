                  "security": "auto"
                }
              ]
            }
          ]
        },
        "streamSettings": {
          "network": "ws",
          "security": "none",
          "wsSettings": {
            "headers": {
              "Host": "dl.kgvn.garenanow.com"
            },
            "path": "/zafkiel"
          }
        },
        "tag": "Zafkiel / VN"
      },
      {
        "protocol": "freedom",
        "tag": "direct"
      },
      {
        "protocol": "freedom",
        "tag": "bypass"
      },
      {
        "protocol": "blackhole",
        "tag": "block"
      },
      {
        "protocol": "dns",
        "settings": {
          "address": "1.1.1.1",
          "userLevel": 1
        },
        "tag": "dns-out"
      }
    ],
    "ping": {
      "disableIPv6": true,
      "protocol": "unprivileged"
    },
    "policy": {
      "levels": {
