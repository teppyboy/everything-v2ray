          "udp": true
        },
        "tag": "socks"
      },
      {
        "listen": "0.0.0.0",
        "port": 9080,
        "protocol": "http",
        "settings": {
          "allowTransparent": true
        },
        "tag": "http"
      },
      {
        "listen": "0.0.0.0",
        "port": 6450,
        "protocol": "dokodemo-door",
        "settings": {
          "address": "1.0.0.1",
          "network": "tcp,udp",
          "port": 53
        },
        "tag": "dns-in"
      }
    ],
    "log": {
      "loglevel": "error"
    },
    "outbounds": [
      {
        "domainStrategy": "UseIPv4",
